{
  "manifest_version": 2,
  "name": "esa notifier",
  "version": "0.0.4",
  "permissions": [
    "https://*.esa.io/*",
    "notifications"
  ],
  "background": {
    "scripts": [
      "js/jquery-2.1.1.min.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon_close_128.png",
    "default_title": "esa notifier",
    "default_popup":  "view/popup.html"
  },
  "icons": {
    "48":  "img/icon_open_48.png",
    "128": "img/icon_open_128.png"
  }
}
